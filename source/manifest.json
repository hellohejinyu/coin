{
  "manifest_version": 2,
  "name": "chrome-coin",
  "version": "1.0.0",
  "description": "比特币价格实时关注插件",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "http://localhost/*",
        "http://127.0.0.1/*",
        "https://localhost/*",
        "https://127.0.0.1/*"
      ],
      "js": [
        "react.production.min.js",
        "react-dom.production.min.js",
        "react-router-dom.min.js",
        "pre.js",
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "https://www.baidu.com"
}